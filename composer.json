{
    "name": "analogue/factory",
    "description": "Implementation of Illuminate database factory for Analogue orm",
    "homepage": "http://github.com/analogueorm/factory",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "remi@code16.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Analogue\\Factory\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/database": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "fzaninotto/faker": "~1.4"
    },
    "require-dev": {
        "analogue/orm": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "phpunit/phpunit": "~5.0",
        "laravel/laravel": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^1.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/FactoryTestCase.php"
        ],
        "psr-4": {
            "Stubs\\": "tests/Stubs"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Analogue\\Factory\\FactoryServiceProvider"
            ]
        }
    }
}
