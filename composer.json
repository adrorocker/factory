{
    "name": "analogue/factory",
    "description": "Implementation of Illuminate database factory for Analogue orm",
    "homepage": "http://github.com/analogueorm/factory",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "remi@code16.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Analogue\\Factory\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.6.12",
        "illuminate/database": ">=5.6.12",
        "fzaninotto/faker": "~1.4"
    },
    "require-dev": {
        "analogue/orm": "5.6.*",
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": ">=5.6.12",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^1.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/FactoryTestCase.php"
        ],
        "psr-4": {
            "Stubs\\": "tests/Stubs"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Analogue\\Factory\\FactoryServiceProvider"
            ]
        }
    }
